<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <EditText android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/edit_string_value">
        <requestFocus></requestFocus>
    </EditText>
    <CheckBox android:text="CheckBox" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/checkBox_boolean_value"></CheckBox>
    <Button android:id="@+id/button1" android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="@string/search" android:onClick="onClickSaveButton"></Button>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/textView1" android:textAppearance="?android:attr/textAppearanceMedium" android:text="TextView"></TextView>
    <Button android:id="@+id/button2" android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="@string/getdata" android:onClick="onClickGetDataButton"></Button>
</LinearLayout>
