<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.dabits.android.nicoplayer"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".activities.SplashActivity"
                  android:label="@string/activity_name_splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.VideoListActivity"
                  android:label="@string/activity_name_video_list">
        </activity>
        <activity android:name=".activities.AccountActivity"
                  android:label="@string/activity_name_account">
        </activity>

    <activity android:name=".activities.PlayActivity"></activity>
<activity android:name=".activities.PreloadActivity"></activity>
</application>
    <uses-sdk android:minSdkVersion="5" />


<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 